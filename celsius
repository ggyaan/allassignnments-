<body>
     <script>
         // fahrenheit to celsius  c=(f-32)/1.8
         // celsius to fahrenheit  f=(c*1.8)+32
         

         function cTof(celsius){
             const ctemp=celsius;
             const ctofareh=ctemp*9/5+32;
             const message=ctofareh;
             console.log(message);
         }
         function fToc(fahrenheit){
             const ftemp=fahrenheit;
             const ftocelsius=(ftemp-32)*5/9;
             const message=ftocelsius;
             console.log(message);
         }
         fToc(40);
         cTof(60);

     </script>
</body>
