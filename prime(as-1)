    <title>Prime Number</title>
</head>
<body>
    <script>
       function isprime(n){
           if(n<2){
               return `${n} is not a prime`;
           }
           for(let i=2;i<n;i++){
               if(n%i==0){
                   return `${n} is not a prime`;
                
               }
           }
           return `${n} is a  prime`;
       }

     console.log(isprime(11));
    </script>
</body>
</html>
