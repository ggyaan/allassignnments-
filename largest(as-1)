<body>
    <script>
        function mNum(x, y, z) 
 {
    maxNum = 0;
  if (x > y)
  {
    maxNum = x;
  } else
  {
    maxNum = y;
  }
  if (z > maxNum) 
  {
    maxNum = z;
  }
  return maxNum;
}

console.log(mNum(1,0,1));
console.log(mNum(0,3,4));
console.log(mNum(1000,510,440));
    </script>
</body>
</html>
